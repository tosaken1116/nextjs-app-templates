{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.addMissingImports": true
  },
  "cSpell.words": [
    "autodocs",
    "commitlint",
    "commitmsg",
    "esnext",
    "hygen",
    "markuplint",
    "nextjs",
    "precommit",
    "tailwindcss"
  ]
}
